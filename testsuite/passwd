# Format:
# [<user>][:<group1>[,<group2>]]
#
# Examples:
#
# # Create a user named andy
# andy
#
# # Create a group named admin
# :admin
#
# # Create a user named andy and add him to admin and wheel groups
# andy:admin,wheel

andy:      cool,     admin

carmen:    cool

bill:      losers,   admin

joe:       losers

bob:       losers

